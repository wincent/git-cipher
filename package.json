{
  "name": "git-cipher",
  "version": "2.0.0-pre.4",
  "bin": "./lib/main.mjs",
  "files": [
    "docs",
    "lib"
  ],
  "description": "Manages encrypted content in a Git repo",
  "exports": null,
  "repository": "https://github.com/wincent/git-cipher",
  "author": "Greg Hurrell <greg@hurrell.net>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@types/node": "*",
    "typescript": "*"
  },
  "scripts": {
    "build": "npx -y esbuild src/main.mts --bundle --format=esm --platform=node --sourcemap --outfile=lib/main.mjs",
    "clean": "rm -r lib/*",
    "compile": "bin/yarn run tsc --noEmit",
    "format:check": "npx -y prettier --check \"**/*.{md,mts}\" \"*.{md,json}\"",
    "format": "npx -y prettier --write \"**/*.{md,mts}\" \"*.{md,json}\""
  },
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  }
}
